            <div>
              <a href=""> A-Blog</a>
              <img src={imageExample} alt="" />
            </div>
            <div>
              <a href=""> EngPa</a>
              <img src={imageExample} alt="" />
            </div>
            <div>
              <a href=""> P-lab</a>
              <img src={imageExample} alt="" />
            </div>
            <div>
              <a href=""> Year - Review</a>
              <img src={imageExample} alt="" />
            </div>
          </div>
        </div>
      </div>
      <Footer />
    </main>
  </body>
</html>
